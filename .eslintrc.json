{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "import/no-unresolved": 0,
        "indent": [
            "error",
            4
        ],
        "semi": [
            "error",
            "never"
        ],
        "max-len":[
            "error",
            200
        ],
        "no-lonely-if": "off",
        "default-case": "off",
        "no-param-reassign": "off",
        "no-use-before-define": "off",
        "no-plusplus": "off"
    }
}